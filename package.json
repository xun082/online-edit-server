{
  "name": "online-edit-server",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "author": "Moment",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production nest build",
    "format": "prettier --write .",
    "format:ci": "prettier --list-different \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch --trace-warnings",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env  NODE_ENV=production node dist/main",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:ci": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "@fastify/multipart": "^8.3.0",
    "@fastify/static": "^7.0.4",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.3.10",
    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/platform-ws": "^10.4.3",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/websockets": "^10.3.10",
    "@supercharge/request-ip": "^1.2.0",
    "@webundsoehne/nest-fastify-file-upload": "^2.2.0",
    "@willsoto/nestjs-prometheus": "^6.0.1",
    "argon2": "^0.40.3",
    "bull": "^4.15.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "fastify": "^4.28.1",
    "http-status-codes": "^2.3.0",
    "husky": "^9.1.3",
    "imagekit": "^5.2.0",
    "ioredis": "^5.4.1",
    "minio": "^8.0.1",
    "mongoose": "^8.5.1",
    "nodemailer": "^6.9.14",
    "passport-jwt": "^4.0.1",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.7.5",
    "svg-captcha": "^1.4.0",
    "uuid": "^10.0.0",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0",
    "y-mongodb-provider": "^0.2.0",
    "y-websocket": "^2.0.4",
    "yjs": "^13.6.19"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@compodoc/compodoc": "^1.1.25",
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.10",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "@types/nodemailer": "^6.4.15",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.4",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "pnpm lint",
      "pnpm format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.0.0"
  },
  "engineStrict": true
}
